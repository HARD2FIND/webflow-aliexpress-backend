# Backend Environment Variables

# Server
PORT=3000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/webflow-aliexpress

# Webflow OAuth
WEBFLOW_CLIENT_ID=your_webflow_client_id
WEBFLOW_CLIENT_SECRET=your_webflow_client_secret
WEBFLOW_REDIRECT_URI=http://localhost:3000/auth/callback

# Security
ENCRYPTION_KEY=your_encryption_key_change_in_production
SESSION_SECRET=your_session_secret_change_in_production

# CORS
CORS_ORIGIN=http://localhost:1337
